<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<!-- 用作白底蓝线 -->
	<item>
		<shape>
			<solid android:color="#00B1FF" />
		</shape>
	</item>
	<item
		android:top="1dp"
		android:left="1dp"
		android:right="1dp"
		android:bottom="1dp">
		<shape>
			<solid android:color="@color/BlackColor" />
		</shape>
	</item>

</layer-list>